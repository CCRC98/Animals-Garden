<!-- pageContent -->
<section class="full-width pageContent">
	<!-- navBar -->
	<div class="full-width navBar">
		<div class="full-width navBar-options">
			<i class="zmdi zmdi-swap btn-menu" id="btn-menu"></i>
			<div class="mdl-tooltip" for="btn-menu">Ocultar / Mostrar Men√∫</div>
			<nav class="navBar-options-list">
				<ul class="list-unstyle">
					
					<li class="btn-exit" id="btn-exit">
						<i class="zmdi zmdi-power"></i>
						<div class="mdl-tooltip" for="btn-exit">Cerrar Sesi√≥n</div>
					</li>
					<li class="text-condensedLight noLink"><small>{{name}}</small></li>
					<li class="noLink">
						<figure>
							<img src="/assets/img/avatar-male.png" alt="Avatar" class="img-responsive">
						</figure>
					</li>
				</ul>
			</nav>
		</div>
	</div>
	<section class="full-width header-well">
		
		<section class="full-width text-center" style="padding: 40px 0;">
			<h3 class="text-condensedLight">Lista de PQRS presentadas en Animal's Garden</h3>
					
		</section>
	</section>
	<div class="full-width divider-menu-h"></div>
	<div class="mdl-grid">
		<div class="mdl-cell mdl-cell--4-col-phone mdl-cell--8-col-tablet mdl-cell--12-col-desktop">
			<div class="table-responsive">
				<table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp full-width table-responsive">
					<thead>
						<tr>
							<th class="mdl-data-table__cell--non-numeric">Categoria</th>
							<th>Usuario</th>
							<th>Mensaje</th>
							<th>Opciones</th>
						</tr>
					</thead>
					<tbody>
						{{#each pqrs}}
							<tr>
								<td class="mdl-data-table__cell--non-numeric">{{id_pqrs}}</td>
								<td>{{nombre}}</td>
								<td>{{mensaje}}</td>
								<td>
									<a href="/vet/pqrs/{{id_pqrs_usr}}">
										<button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
											<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye" viewBox="0 0 16 16">
												<path d="M16 8s-3-5.5-8-5.5S0 8 0 8s3 5.5 8 5.5S16 8 16 8M1.173 8a13 13 0 0 1 1.66-2.043C4.12 4.668 5.88 3.5 8 3.5s3.879 1.168 5.168 2.457A13 13 0 0 1 14.828 8q-.086.13-.195.288c-.335.48-.83 1.12-1.465 1.755C11.879 11.332 10.119 12.5 8 12.5s-3.879-1.168-5.168-2.457A13 13 0 0 1 1.172 8z"/>
												<path d="M8 5.5a2.5 2.5 0 1 0 0 5 2.5 2.5 0 0 0 0-5M4.5 8a3.5 3.5 0 1 1 7 0 3.5 3.5 0 0 1-7 0"/>
											</svg>
										</button>
									</a>
								</td>
							</tr>
						{{else}}
							<tr>
								<td colspan="5">
									<div class="col-md-4 mx-auto">
										<div class="card card-body text-center">
											<p>No hay PQRS registradas aun.</p>
										</div>
									</div>
								</td>
							</tr>
						{{/each}}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</section>